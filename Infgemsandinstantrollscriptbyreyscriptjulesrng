-- ‚ö° GUI Roll + Auto Redeem Kode Jules RNG ‚ö°
-- Created by Rey_Script

if getgenv().AntiMultiExecute then
    return game.StarterGui:SetCore("SendNotification", {
        Title = "‚ö†Ô∏è WARNING ‚ö†Ô∏è";
        Text = "Don't execute more than 1 because you will get banned from the game!";
        Duration = 6;
    })
end
getgenv().AntiMultiExecute = true

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local LocalPlayer = Players.LocalPlayer

-- Remote
local RollRemote = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("Roll")
local RedeemRemote = ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("Redeem")

-- GUI Buat User
local ScreenGui = Instance.new("ScreenGui", LocalPlayer:WaitForChild("PlayerGui"))
ScreenGui.Name = "Rey_GUI"

local Frame = Instance.new("Frame", ScreenGui)
Frame.Size = UDim2.new(0, 300, 0, 220)
Frame.Position = UDim2.new(0.5, -150, 0.5, -110)
Frame.BackgroundColor3 = Color3.fromRGB(180, 220, 255)
Frame.BorderSizePixel = 0
Frame.BackgroundTransparency = 0.2
Frame.Active = true
Frame.Draggable = true

-- UI Corner
local UICorner = Instance.new("UICorner", Frame)
UICorner.CornerRadius = UDim.new(0, 15)

-- Glow efek
local UIStroke = Instance.new("UIStroke", Frame)
UIStroke.Thickness = 2
UIStroke.Color = Color3.fromRGB(100, 180, 255)
UIStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border

-- Judul
local Title = Instance.new("TextLabel", Frame)
Title.Size = UDim2.new(1, 0, 0, 40)
Title.BackgroundTransparency = 1
Title.Text = "‚ö° Jules RNG GUI ‚ö°"
Title.TextColor3 = Color3.fromRGB(60, 80, 120)
Title.Font = Enum.Font.GothamBold
Title.TextSize = 20

-- Tombol Start Loop
local StartBtn = Instance.new("TextButton", Frame)
StartBtn.Size = UDim2.new(0.45, -5, 0, 40)
StartBtn.Position = UDim2.new(0.05, 0, 0.25, 0)
StartBtn.BackgroundColor3 = Color3.fromRGB(173, 216, 230)
StartBtn.Text = "‚ñ∂ Start Loop"
StartBtn.TextColor3 = Color3.fromRGB(50, 70, 100)
StartBtn.Font = Enum.Font.GothamSemibold
StartBtn.TextSize = 16
Instance.new("UICorner", StartBtn).CornerRadius = UDim.new(0, 10)

-- Tombol Stop Loop
local StopBtn = Instance.new("TextButton", Frame)
StopBtn.Size = UDim2.new(0.45, -5, 0, 40)
StopBtn.Position = UDim2.new(0.5, 5, 0.25, 0)
StopBtn.BackgroundColor3 = Color3.fromRGB(255, 150, 150)
StopBtn.Text = "‚èπ Stop Loop"
StopBtn.TextColor3 = Color3.fromRGB(60, 0, 0)
StopBtn.Font = Enum.Font.GothamSemibold
StopBtn.TextSize = 16
Instance.new("UICorner", StopBtn).CornerRadius = UDim.new(0, 10)

-- Delay Box
local DelayBox = Instance.new("TextBox", Frame)
DelayBox.Size = UDim2.new(0.9, 0, 0, 30)
DelayBox.Position = UDim2.new(0.05, 0, 0.55, 0)
DelayBox.BackgroundColor3 = Color3.fromRGB(230, 240, 250)
DelayBox.Text = "0"
DelayBox.TextColor3 = Color3.fromRGB(50, 70, 100)
DelayBox.Font = Enum.Font.Gotham
DelayBox.TextSize = 14
Instance.new("UICorner", DelayBox).CornerRadius = UDim.new(0, 8)

-- Tombol Redeem All Codes
local RedeemBtn = Instance.new("TextButton", Frame)
RedeemBtn.Size = UDim2.new(0.9, 0, 0, 35)
RedeemBtn.Position = UDim2.new(0.05, 0, 0.75, 0)
RedeemBtn.BackgroundColor3 = Color3.fromRGB(150, 200, 255)
RedeemBtn.Text = "üéÅ Redeem All Codes"
RedeemBtn.TextColor3 = Color3.fromRGB(30, 50, 80)
RedeemBtn.Font = Enum.Font.GothamSemibold
RedeemBtn.TextSize = 16
Instance.new("UICorner", RedeemBtn).CornerRadius = UDim.new(0, 10)

-- Status
local Status = Instance.new("TextLabel", Frame)
Status.Size = UDim2.new(1, 0, 0, 25)
Status.Position = UDim2.new(0, 0, 0.9, 0)
Status.BackgroundTransparency = 1
Status.Text = "Status: Idle"
Status.TextColor3 = Color3.fromRGB(70, 100, 150)
Status.Font = Enum.Font.Gotham
Status.TextSize = 14

-- Loop
local running = false
StartBtn.MouseButton1Click:Connect(function()
    if running then return end
    running = true
    Status.Text = "Status: Loop Running..."
    task.spawn(function()
        while running do
            -- pakai args terbaru yang kamu kasih üëá
            local args = {
                buffer.fromstring("\128\000\000\000\bA\179\026\002\000\000\000\000\000\000\128\178\001\241?")
            }
            RollRemote:FireServer(unpack(args))
            
            local delayTime = tonumber(DelayBox.Text) or 0
            task.wait(math.max(0, delayTime))
        end
    end)
end)

StopBtn.MouseButton1Click:Connect(function()
    running = false
    Status.Text = "Status: Stopped"
end)

-- Redeem Codes
local codes = {"RELEASE", "GEMS", "UPDATE1", "FREE" }
RedeemBtn.MouseButton1Click:Connect(function()
    for _, code in ipairs(codes) do
        RedeemRemote:FireServer(code)
        task.wait(0.2)
    end
    Status.Text = "Status: Codes Redeemed"
end)
