-- üåÄ Final Script dengan GUI + Loop + Redeem + Proteksi
if getgenv().ReyScriptExecuted then
	game.StarterGui:SetCore("SendNotification", {
		Title = "‚ö†Ô∏è Don't execute more than 1 ‚ö†Ô∏è",
		Text = "You will get banned if you run script twice!",
		Duration = 6
	})
	return
end
getgenv().ReyScriptExecuted = true

-- Services
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local TweenService = game:GetService("TweenService")
local player = Players.LocalPlayer

-- Anti-lag protect
local loopRunning = false
local loopDelay = 0 -- default 0 detik
local customCodes = {"Release", "FreeGems", "Update1"} -- default daftar kode

-- Create GUI
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Parent = player:WaitForChild("PlayerGui")

-- Background intro
local Intro = Instance.new("Frame")
Intro.Size = UDim2.new(1,0,1,0)
Intro.BackgroundColor3 = Color3.fromRGB(173, 216, 230) -- biru muda
Intro.BackgroundTransparency = 0
Intro.Parent = ScreenGui

local IntroLabel = Instance.new("TextLabel")
IntroLabel.Size = UDim2.new(1,0,1,0)
IntroLabel.BackgroundTransparency = 1
IntroLabel.Text = "‚ú® Created by Rey_Script ‚ú®"
IntroLabel.Font = Enum.Font.Gotham
IntroLabel.TextSize = 40
IntroLabel.TextColor3 = Color3.fromRGB(255,255,255)
IntroLabel.Parent = Intro

-- Fade effect
TweenService:Create(Intro, TweenInfo.new(2, Enum.EasingStyle.Sine), {BackgroundTransparency = 1}):Play()
TweenService:Create(IntroLabel, TweenInfo.new(2, Enum.EasingStyle.Sine), {TextTransparency = 1}):Play()
game:GetService("Debris"):AddItem(Intro, 2.5)

-- Main Frame
local Frame = Instance.new("Frame")
Frame.Size = UDim2.new(0, 250, 0, 250)
Frame.Position = UDim2.new(0.35, 0, 0.3, 0)
Frame.BackgroundColor3 = Color3.fromRGB(200,230,255)
Frame.BorderSizePixel = 0
Frame.Active = true
Frame.Draggable = true
Frame.Parent = ScreenGui

-- UI Corner
local UICorner = Instance.new("UICorner")
UICorner.CornerRadius = UDim.new(0,15)
UICorner.Parent = Frame

-- Title
local Title = Instance.new("TextLabel")
Title.Size = UDim2.new(1,0,0,40)
Title.BackgroundTransparency = 1
Title.Text = "‚ö° Rey Script Hub ‚ö°"
Title.Font = Enum.Font.Gotham
Title.TextSize = 20
Title.TextColor3 = Color3.fromRGB(50,50,50)
Title.Parent = Frame

-- Start Loop Button
local StartButton = Instance.new("TextButton")
StartButton.Size = UDim2.new(0.8,0,0,40)
StartButton.Position = UDim2.new(0.1,0,0.2,0)
StartButton.Text = "‚ñ∂ Start Loop"
StartButton.BackgroundColor3 = Color3.fromRGB(173, 216, 230)
StartButton.TextColor3 = Color3.fromRGB(30,30,30)
StartButton.Font = Enum.Font.Gotham
StartButton.TextSize = 18
StartButton.Parent = Frame

local corner1 = Instance.new("UICorner", StartButton)
corner1.CornerRadius = UDim.new(0,10)

-- Stop Button
local StopButton = Instance.new("TextButton")
StopButton.Size = UDim2.new(0.8,0,0,40)
StopButton.Position = UDim2.new(0.1,0,0.4,0)
StopButton.Text = "‚èπ Stop Loop"
StopButton.BackgroundColor3 = Color3.fromRGB(173, 216, 230)
StopButton.TextColor3 = Color3.fromRGB(30,30,30)
StopButton.Font = Enum.Font.Gotham
StopButton.TextSize = 18
StopButton.Parent = Frame

local corner2 = Instance.new("UICorner", StopButton)
corner2.CornerRadius = UDim.new(0,10)

-- Redeem Button
local RedeemButton = Instance.new("TextButton")
RedeemButton.Size = UDim2.new(0.8,0,0,40)
RedeemButton.Position = UDim2.new(0.1,0,0.6,0)
RedeemButton.Text = "üéÅ Redeem Codes"
RedeemButton.BackgroundColor3 = Color3.fromRGB(173, 216, 230)
RedeemButton.TextColor3 = Color3.fromRGB(30,30,30)
RedeemButton.Font = Enum.Font.Gotham
RedeemButton.TextSize = 18
RedeemButton.Parent = Frame

local corner3 = Instance.new("UICorner", RedeemButton)
corner3.CornerRadius = UDim.new(0,10)

-- Input Box Codes
local CodeBox = Instance.new("TextBox")
CodeBox.Size = UDim2.new(0.8,0,0,40)
CodeBox.Position = UDim2.new(0.1,0,0.8,0)
CodeBox.PlaceholderText = "Pisahkan kode dengan koma"
CodeBox.Text = ""
CodeBox.BackgroundColor3 = Color3.fromRGB(240,240,255)
CodeBox.TextColor3 = Color3.fromRGB(0,0,0)
CodeBox.Font = Enum.Font.Gotham
CodeBox.TextSize = 16
CodeBox.Parent = Frame

local corner4 = Instance.new("UICorner", CodeBox)
corner4.CornerRadius = UDim.new(0,10)

-- FUNCTIONS
local function roll()
	local args = {
		buffer.fromstring("\128\000\000\000\bA\179\026\002\000\000\000\000\000\000\128\178\001\241?")
	}
	ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("Roll"):FireServer(unpack(args))
end

StartButton.MouseButton1Click:Connect(function()
	if not loopRunning then
		loopRunning = true
		while loopRunning do
			roll()
			task.wait(loopDelay)
		end
	end
end)

StopButton.MouseButton1Click:Connect(function()
	loopRunning = false
end)

RedeemButton.MouseButton1Click:Connect(function()
	if CodeBox.Text ~= "" then
		customCodes = {}
		for code in string.gmatch(CodeBox.Text, '([^,]+)') do
			table.insert(customCodes, code)
		end
	end
	for _, code in ipairs(customCodes) do
		ReplicatedStorage.Remotes.Redeem:FireServer(code)
		task.wait(0.2)
	end
end)
